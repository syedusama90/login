
<% if userr_signed_in? %>
<h1>Users List</h1>

<table class="table table-dark table-striped table-bordered">
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th class = "col-span "3" >Email</th>
      <th>Phone</th>
    </tr>
  </thead>

  <tbody>
      <tr>
        <td><%= current_userr.fname %></td>
        <td><%= current_userr.lname %></td>
        <td><%= current_userr.email %></td>
        <td><%= current_userr.phone %></td>
      </tr>
  </tbody>
</table>
<%= link_to 'Back', :back ,class:"btn btn-outline-light btn-lg px-5" %>
<% end %>